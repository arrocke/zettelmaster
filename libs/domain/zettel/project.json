{
  "name": "domain-zettel",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/domain/zettel/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/domain/zettel",
        "main": "libs/domain/zettel/src/index.ts",
        "tsConfig": "libs/domain/zettel/tsconfig.lib.json",
        "assets": ["libs/domain/zettel/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/domain/zettel/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/domain/zettel"],
      "options": {
        "jestConfig": "libs/domain/zettel/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
