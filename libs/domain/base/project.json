{
  "name": "domain-base",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/domain/base/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/domain/base",
        "main": "libs/domain/base/src/index.ts",
        "tsConfig": "libs/domain/base/tsconfig.lib.json",
        "assets": ["libs/domain/base/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/domain/base/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/domain/base"],
      "options": {
        "jestConfig": "libs/domain/base/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
